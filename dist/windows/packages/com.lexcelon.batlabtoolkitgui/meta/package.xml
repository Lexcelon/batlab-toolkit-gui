<?xml version="1.0" encoding="UTF-8"?>
<Package>
  <DisplayName>Batlab Toolkit GUI</DisplayName>
  <Description>Install the Batlab Toolkit GUI application.</Description>
  
  <!-- Please leave the below variable as 0.0.0
       For local dev builds, the version will just be 0.0.0
       For official builds on the build server, this gets replaced with the build number
  -->
  <Version>0.0.0</Version>

  <!-- Please leave the below variable as 2000-01-01
       For local dev builds, the date will just be 2000-01-01
       For official builds on the build server, this gets replaced with the date
  -->
  <ReleaseDate>2000-01-01</ReleaseDate>
  
  <UpdateText>See https://github.com/Lexcelon/batlab-toolkit-gui/releases for release notes and development history.</UpdateText>
  <Licenses>
    <License name="GNU General Public License" file="COPYING" />
  </Licenses>
  <ForcedInstallation>true</ForcedInstallation>
  <Script>installscript.js</Script>
</Package>
